{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "DATABASE_URL": "@your-postgres-url",
    "MODEL_URL": "@your-model-url",
    "SECRET_KEY": "@your-secret-key",
    "PYTHON_VERSION": "3.9"
  }
}
